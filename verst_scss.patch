+    display: flex;
+    border-radius: 5px;
+    position: relative;
+    align-items: center;
+    justify-content: center;
+    background-color: #e5ebf1;
+
+    &:hover {
+      background-color: #d6e5f5;
+    }
+  }
+
+  &__btn-list {
+    padding-top: 10px;
+  }
+
+  &__btn-text {
+    color: #55677d;
+    text-align: center;
+    text-decoration: none;
+  }
+
+  &__btn-list-element {
+    height: 36px;
+    display: flex;
+    color: #55677d;
+    font-size: 13px;
+    padding-left: 16px;
+    padding-right: 16px;
+    align-items: center;
+    text-decoration: none;
+
+    &:hover {
+      text-decoration: none;
+      background-color: #aeb7c21f;
+    }
+  }
+
+  &__icon {
+    font-size: 20px;
+  }
+}
+
+.header {
+  padding: 0 0 7px;
+  position: relative;
+  border-bottom: 1px solid #dce1e6;
+
+  &__name {
+    font-size: 19px;
+    font-weight: 400;
+    line-height: 25px;
+    padding-left: 1px;
+    margin: -1px 0 -1px -1px;
+  }
+
+  &__check-online {
+    top: 0;
+    padding: 0;
+    right: 10px;
+    font-size: 12px;
+    margin: 5px 0 0 0;
+    text-align: center;
+    position: absolute;
+  }
+
+  &__status {
+    width: 100%;
+    margin: 5px 0;
+    display: block;
+    font-size: 13px;
+    line-height: 16px;
+  }
+}
+
+.main {
+  &__list {
+    padding: 0;
+  }
+
+  &__list-element {
+    height: 30px;
+    display: flex;
+    justify-content: end;
+
+    &_text {
+      width: 215px;
+      color: #84909c;
+    }
+
+    &_link {
+      color: #2a5885;
+      cursor: pointer;
+      text-decoration: none;
+    }
+  }
+
+  &__text-btn {
+    text-align: center;
+    margin: 20px 0 10px 0;
+  }
+
+    &_link {
+      color: #2a5885;
+      cursor: pointer;
+      text-decoration: none;
+    }
+  }
+
+.footer {
+  padding-top: 10px;
+  border-top: 1px solid #dce1e6;
+
+  &__list {
+    display: flex;
+    padding: 0 50px;
+    list-style: none;
+    justify-content: space-between;
+  }
+
+  &__list-element {
+    display: flex;
+    align-items: center;
+  }
+
+  &__count {
+    margin: 0;
+    font-size: 19px;
+    text-align: center;
+  }
+
+  &__text {
+    margin: 0;
+  }
+}
+
+.photo-block {
+  display: flex;
+  justify-content: space-between;
+
+  &__info {
+    display: flex;
+  }
+
+  &__text {
+    margin-right: 5px;
+
+    &_gray {
+      color: #84909c;
+      margin: auto 0;
+    }
+  }
+
+  &__photo-list {
+    display: flex;
+    justify-content: space-around;
+  }
+
+  &__photo-element {
+    list-style: none;
+  }
+
+  &__photo {
+    width: 123px;
+    height: 123px;
+  }
+}
